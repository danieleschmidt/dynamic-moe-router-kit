{
  "dashboard": {
    "title": "Dynamic MoE Router - Production Dashboard",
    "description": "Comprehensive monitoring for Dynamic MoE Router in production",
    "tags": ["moe", "routing", "ml", "performance"],
    "timezone": "utc",
    "panels": [
      {
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(moe_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "value"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        }
      },
      {
        "title": "Average Response Time",
        "type": "stat", 
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(moe_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th percentile (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {
                  "color": "green", 
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red", 
                  "value": 500
                }
              ]
            }
          }
        }
      },
      {
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(moe_requests_total{status=\"error\"}[5m]) / rate(moe_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow", 
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        }
      },
      {
        "title": "Expert Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "moe_expert_utilization",
            "legendFormat": "Expert {{expert_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 5,
              "showPoints": "never",
              "stacking": {
                "group": "A",
                "mode": "none"
              }
            },
            "unit": "percent"
          }
        }
      },
      {
        "title": "FLOP Reduction",
        "type": "timeseries", 
        "targets": [
          {
            "expr": "moe_flop_reduction_percent",
            "legendFormat": "FLOP Reduction %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes"
          }
        }
      },
      {
        "title": "Cache Hit Rate", 
        "type": "gauge",
        "targets": [
          {
            "expr": "moe_cache_hits / (moe_cache_hits + moe_cache_misses) * 100",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green", 
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "title": "Load Balance Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "moe_load_balance_score",
            "legendFormat": "Load Balance Quality"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 0.8
                }
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}